<% extend 'layout.ect' %>
	<% block 'body': %>
    <% if @user: %>
      <h3>Hello <%- @user.displayName %>!</h3>
      <p>Your level is <%- @user.level %>.</p>
    <% end %>

    <ul>
      <% for projectName, projectDetails of @projects: %>
    		<li><a href="/project/<%- projectName %>"><%- projectName %></a></li>
      <% end %>
    </ul>
	<% end %>
<% end %>
